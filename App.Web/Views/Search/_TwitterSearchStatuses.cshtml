@model IEnumerable<TweetSharp.TwitterSearchStatus>
    @foreach (var item in Model)
    {
    <div class="tweet">
        <img src = "@item.Author.ProfileImageUrl" alt = "@item.Author.ScreenName" title = "@item.Author.ScreenName" />
        <a href="https://twitter.com/@(item.Author.ScreenName)" class="author">
            <span class="author">@item.FromUserName</span>&nbsp;<span class="authorId">&#64;@item.Author.ScreenName</span>
        </a>
        <div class="text">@Html.Raw(item.TextAsHtml)</div>
    </div>
    }