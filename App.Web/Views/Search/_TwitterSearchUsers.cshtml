@model IEnumerable<TweetSharp.TwitterUser>
@foreach (var item in Model)
{
    <div class="tweet">
        <img src = "@item.ProfileImageUrl" alt = "@item.ScreenName" title = "@item.ScreenName" />
        <a href="https://twitter.com/@(item.ScreenName)" class="author">
            <span class="author">@item.Name</span>&nbsp;<span class="authorId">&#64;@item.ScreenName</span>
        </a>
        <div class="text">@Html.Raw(item.Description)</div>
    </div>
}